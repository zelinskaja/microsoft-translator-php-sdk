{
    "name": "wowmaking/microsoft-translator-php-sdk",
    "type": "library",
    "description": "Microsoft Translator API V3 PHP SDK",
    "keywords": ["translate","Microsoft Translator", "Bing"],
    "homepage": "https://github.com/wowmaking/microsoft-translator-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Wowmaking",
            "email": "info@wowmaking.net",
            "homepage": "http://wowmaking.net"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzle/guzzle": "^3.9",
        "ext-curl": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Wowmaking\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^0.9",
        "satooshi/php-coveralls": "^2.0"
    }
}